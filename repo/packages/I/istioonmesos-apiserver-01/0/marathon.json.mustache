{
    "labels": {
      "HAPROXY_GROUP": "{{istioonmesos-apiserver.l_haproxy_group}}",
      "HAPROXY_0_VHOST": ""
    },
    "id": "{{istioonmesos-apiserver.name}}",
    "backoffFactor": 1.15,
    "backoffSeconds": 1,
    "cmd": "kube-apiserver --etcd-servers http://{{istioonmesos-apiserver.etcd_conf}}:31379 --service-cluster-ip-range 10.99.0.0/16 --insecure-port 8080 -v 2 --insecure-bind-address 0.0.0.0",
    "container": {
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 31080,
          "labels": {
            "VIP_0": "/apiserver:10080"
          },
          "protocol": "tcp",
          "servicePort": 10001
        }
      ],
      "type": "DOCKER",
      "volumes": [],
      "docker": {
        "image": "{{resource.assets.container.docker.apiserver}}",
        "forcePullImage": false,
        "privileged": false,
        "parameters": []
      }
    },
    "cpus": {{istioonmesos-apiserver.cpus}},
    "disk": 0,
    "instances": 1,
    "maxLaunchDelaySeconds": 3600,
    "mem": {{istioonmesos-apiserver.mem}},
    "gpus": 0,
    "networks": [
      {
        "mode": "container/bridge"
      }
    ],
    "requirePorts": false,
    "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 1
    },
    "killSelection": "YOUNGEST_FIRST",
    "unreachableStrategy": {
      "inactiveAfterSeconds": 0,
      "expungeAfterSeconds": 0
    },
    "healthChecks": [],
    "fetch": [],
    "constraints": []
}